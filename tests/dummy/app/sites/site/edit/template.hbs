<div class="row">
  <div class="col-sm-6">
    <h3>Edit Entry</h3>
    <div class="form-group">
      <label for="">Domain</label>
      {{input value=model.attributes.domain class='form-control' placeholder="Domain"}}
    </div>
    <div class="form-group">
      <label for="">Site Id</label>
      {{input value=model.attributes.siteId class='form-control' placeholder="Site Id"}}
    </div>
    <div class="form-group">
      <label for="">Site Name</label>
      {{input value=model.attributes.siteTitle class='form-control' placeholder="Site Name"}}
    </div>
    <div class="form-group">
      <label for="">Client Key</label>
      {{input value=model.attributes.clientKey class='form-control' placeholder="Site Name"}}
    </div>
    <button type="button" class="btn btn-primary" {{action 'save'}}>Save</button>
  </div>
  <div class="col-sm-6">
    <h3>Status</h3>
    {{status}}
  </div>
</div>
