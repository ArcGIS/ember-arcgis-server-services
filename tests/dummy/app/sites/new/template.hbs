<div class="row">
  <div class="col-sm-6">
    <h3>New Entry</h3>
    <div class="form-group">
      <label for="">DOMAIN</label>
      {{input value=domain class="form-control" placeholder="Domain"}}
    </div>
    <div class="form-group">
      <label for="">Site Id</label>
      {{input value=siteId class="form-control" placeholder="Site Id"}}
    </div>
    <div class="form-group">
      <label for="">Site Name</label>
      {{input value=siteName class="form-control" placeholder="Site Name"}}
    </div>
    <div class="form-group">
      <label for="">Client Key</label>
      {{input value=clientKey class="form-control" placeholder="Site Name"}}
    </div>
    <button type="button" class="btn btn-primary" {{action "save"}}>Save</button>
  </div>
  <div class="col-sm-6">
    <h3>Status</h3>
    {{status}}
  </div>
</div>
